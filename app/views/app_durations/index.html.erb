<p id="notice"><%= notice %></p>

<h1>App Durations</h1>

<table>
  <thead>
    <tr>
      <th>Duration</th>
      <th>Durationprice</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @app_durations.each do |app_duration| %>
      <tr>
        <td><%= app_duration.duration %></td>
        <td><%= app_duration.durationPrice %></td>
        <td><%= link_to 'Show', app_duration %></td>
        <td><%= link_to 'Edit', edit_app_duration_path(app_duration) %></td>
        <td><%= link_to 'Destroy', app_duration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App Duration', new_app_duration_path %>
