<p id="notice"><%= notice %></p>

<h1>App Addresses</h1>

<table>
  <thead>
    <tr>
      <th>Hometype</th>
      <th>Homeaddress</th>
      <th>Suitenumber</th>
      <th>State</th>
      <th>City</th>
      <th>Zipcode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @app_addresses.each do |app_address| %>
      <tr>
        <td><%= app_address.homeType %></td>
        <td><%= app_address.homeAddress %></td>
        <td><%= app_address.suiteNumber %></td>
        <td><%= app_address.state %></td>
        <td><%= app_address.city %></td>
        <td><%= app_address.zipcode %></td>
        <td><%= link_to 'Show', app_address %></td>
        <td><%= link_to 'Edit', edit_app_address_path(app_address) %></td>
        <td><%= link_to 'Destroy', app_address, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App Address', new_app_address_path %>
