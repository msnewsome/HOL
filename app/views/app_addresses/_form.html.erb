<%= form_with(model: app_address, local: true) do |form| %>
  <% if app_address.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(app_address.errors.count, "error") %> prohibited this app_address from being saved:</h2>

      <ul>
        <% app_address.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :homeType %>
    <%= form.text_field :homeType %>
  </div>

  <div class="field">
    <%= form.label :homeAddress %>
    <%= form.text_field :homeAddress %>
  </div>

  <div class="field">
    <%= form.label :suiteNumber %>
    <%= form.text_field :suiteNumber %>
  </div>

  <div class="field">
    <%= form.label :state %>
    <%= form.text_field :state %>
  </div>

  <div class="field">
    <%= form.label :city %>
    <%= form.text_field :city %>
  </div>

  <div class="field">
    <%= form.label :zipcode %>
    <%= form.number_field :zipcode %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
